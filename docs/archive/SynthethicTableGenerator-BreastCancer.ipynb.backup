    "        \"batch_size\": trial.suggest_categorical(\"batch_size\", [64, 128, 256, 512]),  # Training batch size\n",
    "        \"learning_rate\": trial.suggest_loguniform(\"learning_rate\", 1e-5, 1e-2),  # Learning rate\n",
